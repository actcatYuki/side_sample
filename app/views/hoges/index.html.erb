<h1>Listing hoges</h1>

<table>
  <thead>
    <tr>
      <th>Hey</th>
      <th>Index</th>
      <th>Hello</th>
      <th>Bye</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hoges.each do |hoge| %>
      <tr>
        <td><%= hoge.hey %></td>
        <td><%= hoge.index %></td>
        <td><%= hoge.hello %></td>
        <td><%= hoge.bye %></td>
        <td><%= link_to 'Show', hoge %></td>
        <td><%= link_to 'Edit', edit_hoge_path(hoge) %></td>
        <td><%= link_to 'Destroy', hoge, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Hoge', new_hoge_path %>
